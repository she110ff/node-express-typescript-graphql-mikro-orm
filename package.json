{
  "name": "career-sherpa-backend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/she110ff/career-sherpa-backend.git",
  "author": "she110ff <youngsoo.j@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "./run_local.sh",
    "start:refresh": "./run_local_refresh.sh",
    "dev": "ts-node-dev ./src/index.ts",
    "build": "tsc",
    "clean": "docker compose --env-file .env down && rm -rf dist && sleep 2 && rm -rf data",
    "dev-db": "docker compose --env-file .env up -d db",
    "clean:dev": "docker compose --env-file .env.dev down && rm -rf dist && sleep 2 && rm -rf data",
    "schema:dev": "docker compose --env-file .env.dev up -d db",
    "migration-up": " npx mikro-orm  migration:up",
    "migration-create": " npx mikro-orm  migration:create",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.245.0",
    "@mikro-orm/cli": "^5.6.6",
    "@mikro-orm/core": "^5.6.6",
    "@mikro-orm/migrations": "^5.6.6",
    "@mikro-orm/postgresql": "^5.6.6",
    "@mikro-orm/seeder": "5.6.6",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@types/nodemailer": "^6.4.7",
    "apollo-server-express": "^3.11.1",
    "argon2": "^0.30.3",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "express-promise-router": "^4.1.1",
    "google-auth-library": "^8.7.0",
    "graphql": "^15.3.0",
    "graphql-upload-ts": "^2.0.5",
    "graphql-ws": "^5.11.2",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.1",
    "sharp": "^0.31.3",
    "shortid": "^2.2.16",
    "ts-node": "^10.9.1",
    "type-graphql": "^1.2.0-rc.1",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.4",
    "prettier": "^2.5.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "tslint": "6.1.3"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
